<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>NodeNews</title>
    <link href="https://fonts.googleapis.com/css2?family=Source+Sans+Pro&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="/css/card.css">
</head>
<body>
    <header>
        <div class="nav">
            <ul>
              <li><a href="/" class="nav-link">Top headlines</a></li>
              <li><a href="/country" class="nav-link">Country</a></li>
              <li><a href="/category" class="nav-link">Category</a></li>
            </ul>
          </div>
    </header>
    <div class="cards-list">
        <%cateogoryList.forEach((value)=>{ %>
            <a href="category/<%-value%>">
                <div class="card 1">
                    <div class="card_image"> <img src="https://i.redd.it/b3esnz5ra34y.jpg" /></div>
                    <div class="card_title title-white">
                      <p><%-value.toUpperCase()%></p>
                    </div>
                  </div>
            </a>
        <%})%>
        </div>
</body>
</html>